{
  "name": "@wexample/js-helpers",
  "version": "0.0.23",
  "description": "Modern, type-safe helper utilities for Wexample projects.",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "lint": "npm run typecheck",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "wexample",
    "helpers",
    "utility",
    "kebab-case"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "repository": "https://github.com/wexample/js-helpers",
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}